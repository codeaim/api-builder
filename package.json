{
  "name": "@codeaim/api-builder",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/codeaim/api-builder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeaim/api-builder"
  },
  "author": "codeaim",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "npm run format:fix && tsc --project tsconfig.json",
